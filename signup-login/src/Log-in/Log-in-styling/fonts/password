 <script src="https://cdn.evbstatic.com/s3-build/1234159-rc2023-04-13_16.04-d74c276/django/js/require_built/eb/apps/activation/app.js" crossorigin="anonymous" async defer></script>
    <script>
        var ENTER_KEY_CODE = 13;

        document.onkeydown = function (evt) {
            var keyCode = evt ? (evt.which ? evt.which : evt.keyCode) : event.keyCode;
            var eventTarget = evt ? evt.target : event.srcElement;
            var textField = eventTarget.type == 'text' || eventTarget.type == 'password' || eventTarget.type == 'checkbox';

            if (
                keyCode == ENTER_KEY_CODE &&
                textField &&
                eventTarget.form.name
            ) {
                switch(eventTarget.form.name) {
                    case "changepassform":
                        document.forms[eventTarget.form.name].submit();
                    default: break;
                }
            }
        };
    </script>


        

        
        
